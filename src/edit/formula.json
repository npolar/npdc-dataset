{
  "fieldsets": [{
    "title": "Dataset",
    "fields": [{
        "id": "title"
      }, {
        "id": "summary",
        "title": "summary",
        "type": "textarea"
      },
      "progress",
      "draft"
    ]
  }, {
    "title": "Texts",
    "fields": [{
        "id": "released",
        "title": "release date"
      }, {
        "id": "licences",
        "title": "chose a licence",
        "enum": ["http://data.norge.no/nlod/no/1.0", "http://creativecommons.org/licenses/by/3.0/no/", "http://opendatacommons.org/licenses/by/1.0/", "http://lovdata.no/dokument/NL/lov/1961-05-12-2", "http://creativecommons.org/publicdomain/zero/1.0/"]
      },
      "rights",
      "restrictions", {
        "id": "quality",
        "title": "data quality"
      },
      "citation"
    ]
  }, {
    "title": "Classification",
    "fields": [{
        "id": "topics"
      },
      "iso_topics",
      {
        "id": "sets",
        "title": "choose a set",
        "enum": ["arctic", "antarctic", "gcmd.nasa.gov", "cryoclim.net", "ipy.org","docipy"]
      },
       {
        "id": "tags",
        "title": "choose tags",
        "multiple": true,
        "autocomplete": true
      }
    ]
  }, {
    "title": "Links",
    "fields": [{
      "id": "links",
      "fields": [
        {
          "id": "rel",
          "title": "relation to dataset",
          "enum": ["related",
            "service",
            "metadata",
            "project",
            "publication",
            "datacentre",
            "parent",
            "internal",
            "doi",
            "data"
          ]
        }, {
          "id": "href",
          "title": "link"
        },
        "!hreflang",
        "!acccessed",
        "!length",
        "!etag",
         {"id": "type",
         "title": "type",
        "autocomplete": true
        },
        "!modified"
      ]
    }]
  }, {
    "title": "Coverage",
    "fields": [{
        "id": "activity",
        "title": "data collection period"
      },
      "coverage",
      "placenames"
    ]
  }, {
    "title": "People",
    "fields": [{
      "id": "people",
      "fields": [{
          "id": "first_name",
          "title": "first name",
          "autocomplete": "//api.npolar.no/person/?fields=first_name,last_name,organisation,email&format=json&variant=array"
        }, {
          "id": "last_name",
          "title": "last name",
          "autocomplete": "//api.npolar.no/person/?fields=first_name,last_name,organisation,email&format=json&variant=array"
        },
        "!id"
      ]
    }]
  }, {
    "title": "Organisations",
    "fields": [{
      "id": "organisations",
      "fields": [
      "!id",
      {
          "id": "first_name",
          "title": "first name"
        }, {
          "id": "last_name",
          "title": "last name"
        },
        "email",
        {"id": "gcmd_short_name",
         "title": "GCMD short name",
        "autocomplete": true
        },
        "roles",
        "homepage"
      ]
    }]
  },
  {
    "title": "External",
    "fields": [{
      "id": "gcmd",
      "fields": [
      "!locations",
      "!projects",
      "!resolutions",
      "!disciplines",
      "!idn_nodes",
      "!paleo_temporal_coverage",
      "!instruments",
      "!references",
      "!extended",
      "!citation",
      {
        "id": "sciencekeywords",
        "title": "science keywords",
        "fields": [
             {
            "id": "Category",
            "title": "category"
            },
            {
            "id": "Topic",
            "title": "topic"
            },
            {
            "id": "Term",
            "title": "term"
            },
            {
            "id": "Variable_Level_1",
            "title": "variable level 1"
            },
            {
            "id": "Variable_Level_2",
            "title": "variable level 2"
            },
            {
            "id": "Variable_Level_3",
            "title": "variable level 3"
            },
            {
            "id": "Detailed_Variable",
            "title": "detailed variable"
            }
        ]
      }
      ]
    }]
  },
  {
    "title": "Edits",
    "fields": [
      "created",
       {
        "id": "created_by",
        "title": "created by"
      },
      "updated",
      {
        "id": "updated_by",
        "title": "updated by"
      },
      "comment",
      {
        "id": "changes",
        "title": "changes for major updates"
      }
    ]
  }]
}
